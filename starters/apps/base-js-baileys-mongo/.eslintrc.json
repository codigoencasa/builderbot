{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:builderbot/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["builderbot"],
  "rules": {
    "no-unsafe-optional-chaining": "off",
    "import/no-duplicates": "off"
  }
}